{
  "name": "nossa-maternidade-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "expo": {
    "install": {
      "exclude": [
        "jest",
        "@types/jest",
        "@sentry/react-native"
      ]
    }
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --port 8082",
    "build": "npm run build:android",
    "build:web": "expo export:web",
    "build:dev": "eas build --platform all --profile development",
    "build:preview": "eas build --platform all --profile preview",
    "build:staging": "eas build --platform all --profile staging",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:production": "eas build --platform all --profile production",
    "build:prod": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "submit:all": "eas submit --platform all --profile production",
    "update": "eas update --branch production --message",
    "update:prod": "eas update --branch production",
    "prebuild": "expo prebuild --clean",
    "type-check": "tsc --noEmit",
    "test": "node --max-old-space-size=4096 node_modules/.bin/jest",
    "test:dev": "node --max-old-space-size=4096 node_modules/.bin/jest --coverage --coverageThreshold='{}'",
    "test:ci": "node --max-old-space-size=4096 node_modules/.bin/jest --ci --coverage --passWithNoTests --maxWorkers=2 --runInBand --reporters=default",
    "test:watch": "node --max-old-space-size=4096 node_modules/.bin/jest --watch",
    "test:coverage": "node --max-old-space-size=4096 node_modules/.bin/jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:analyze": "node scripts/reduce-eslint-warnings.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "validate:android": "node scripts/validate-android.js",
    "prepare:assets": "ts-node scripts/prepare-assets.ts",
    "prebuild:android": "npm run validate:android",
    "check-ready": "pwsh -File scripts/check-ready.ps1",
    "check-ready:bash": "bash scripts/check-ready.sh",
    "diagnose:powershell": "pwsh -ExecutionPolicy Bypass -File scripts/diagnose-powershell.ps1",
    "diagnose:production": "npx tsc scripts/diagnose-production-readiness.ts --outDir scripts --module commonjs --esModuleInterop --skipLibCheck --target es2017 --lib es2017 && node scripts/diagnose-production-readiness.js",
    "fix:claude-bash": "pwsh -ExecutionPolicy Bypass -File scripts/fix-claude-code-bash.ps1",
    "verify:claude-bash": "pwsh -ExecutionPolicy Bypass -File scripts/verify-claude-code-bash.ps1",
    "verify:cursor": "node scripts/verify-cursor-setup.js",
    "validate:env": "node scripts/validate-env.js",
    "generate:types": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.generated.ts",
    "generate:types:local": "npx supabase gen types typescript --local > src/types/supabase.generated.ts",
    "test:connection": "node scripts/test-connection.js",
    "validate:gemini-secret": "node scripts/validate-gemini-secret.js",
    "test:gemini-edge": "node scripts/test-gemini-edge-function.js",
    "deploy:gemini": "node scripts/deploy-gemini-edge-function.js",
    "deploy:gemini:skip-test": "node scripts/deploy-gemini-edge-function.js --skip-test",
    "validate:design": "node scripts/validate-design-tokens.js",
    "activate:quality-agents": "node scripts/activate-quality-agents.js",
    "check:virtualization": "pwsh -ExecutionPolicy Bypass -File scripts/check-virtualization-status.ps1",
    "fix:virtualization": "pwsh -ExecutionPolicy Bypass -File scripts/fix-docker-virtualization.ps1",
    "validate:platform": "ts-node scripts/validate-platform-design.ts",
    "validate:pre-deploy": "ts-node scripts/validate-pre-deploy.ts",
    "design:refactor": "ts-node scripts/design-refactor.ts",
    "design:validate": "ts-node scripts/design-refactor.ts --validate",
    "validate": "npm run type-check && npm run validate:design && npm run validate:env || echo '⚠️  validate:env falhou (arquivo .env pode não existir)' && npm run lint || echo '⚠️  lint falhou'",
    "health-check": "ts-node scripts/check-project-health.ts",
    "tunnel:web": "pwsh -ExecutionPolicy Bypass -File scripts/start-ngrok-web.ps1",
    "web:tunnel": "pwsh -ExecutionPolicy Bypass -File scripts/start-web-with-tunnel.ps1",
    "precommit": "npm run validate:design && npm run type-check && npm run lint",
    "setup:hooks": "node scripts/setup-hooks.js",
    "prepare": "husky",
    "clean": "pwsh -ExecutionPolicy Bypass -File limpar-projeto-completo.ps1",
    "clean:auto": "pwsh -ExecutionPolicy Bypass -File limpar-projeto-completo.ps1 -AutoConfirm",
    "clean:no-reinstall": "pwsh -ExecutionPolicy Bypass -File limpar-projeto-completo.ps1 -SkipReinstall",
    "clean:old-projects": "pwsh -ExecutionPolicy Bypass -File limpar-projetos-antigos.ps1",
    "clean:old-projects:auto": "pwsh -ExecutionPolicy Bypass -File limpar-projetos-antigos.ps1 -AutoConfirm",
    "clean:files": "pwsh -ExecutionPolicy Bypass -File limpar-arquivos-especificos.ps1",
    "clean:files:auto": "pwsh -ExecutionPolicy Bypass -File limpar-arquivos-especificos.ps1 -AutoConfirm",
    "clean:cursor": "pwsh -ExecutionPolicy Bypass -File remover-cursor-duplicado.ps1",
    "clean:cursor:force": "pwsh -ExecutionPolicy Bypass -File remover-cursor-duplicado.ps1 -Force",
    "organize:downloads": "pwsh -ExecutionPolicy Bypass -File organizar-downloads.ps1",
    "organize:downloads:dry": "pwsh -ExecutionPolicy Bypass -File organizar-downloads.ps1 -DryRun",
    "organize:downloads:auto": "pwsh -ExecutionPolicy Bypass -File organizar-downloads.ps1 -AutoConfirm",
    "clean:duplicates": "pwsh -ExecutionPolicy Bypass -File remover-pastas-duplicadas-downloads.ps1",
    "clean:duplicates:auto": "pwsh -ExecutionPolicy Bypass -File remover-pastas-duplicadas-downloads.ps1 -AutoConfirm",
    "clean:downloads:large": "pwsh -ExecutionPolicy Bypass -File remover-pastas-downloads-grande.ps1",
    "clean:downloads:large:auto": "pwsh -ExecutionPolicy Bypass -File remover-pastas-downloads-grande.ps1 -AutoConfirm"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@expo/vector-icons": "^15.0.3",
    "@google/generative-ai": "^0.24.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.8.6",
    "@react-navigation/native": "^7.1.21",
    "@react-navigation/native-stack": "^7.6.4",
    "@sentry/react-native": "^7.7.0",
    "@shopify/flash-list": "^2.0.2",
    "@supabase/supabase-js": "^2.84.0",
    "@tanstack/react-query": "^5.90.11",
    "base64-arraybuffer": "^1.0.2",
    "expo": "~54.0.25",
    "expo-av": "~16.0.7",
    "expo-blur": "^15.0.7",
    "expo-constants": "^18.0.10",
    "expo-dev-client": "^6.0.18",
    "expo-file-system": "^19.0.19",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.9",
    "expo-localization": "~17.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-speech": "^14.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-tracking-transparency": "~6.0.7",
    "immer": "^11.0.1",
    "lucide-react-native": "^0.554.0",
    "nativewind": "^4.2.1",
    "openai": "^6.9.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^2.0.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.5.1",
    "text-encoding": "^0.7.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-constant-elements": "^7.27.1",
    "@babel/plugin-transform-react-inline-elements": "^7.27.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@typescript-eslint/typescript-estree": "^8.48.1",
    "babel-preset-expo": "^54.0.7",
    "dotenv": "^16.6.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-react-native-a11y": "^3.5.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.13",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "shadcn": "^3.5.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
